;
; BIND data file for fpad domain
;
$TTL    604800
@       IN      SOA     dns.fpad.com. root.fpad.com. (
                              2         ; Serial
                         604800         ; Refresh
                          86400         ; Retry
                        2419200         ; Expire
                         604800 )       ; Negative Cache TTL
;
@       IN      NS      dns.fpad.com.
@       IN      MX  10  mail.fpad.com.
dns 	IN   	A  	192.168.1.160
mail    IN   	A  	192.168.1.161
ftp     IN      A       192.168.1.162
www     IN      A       192.168.1.163
alumno  IN      CNAME   www

